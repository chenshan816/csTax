<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.cs.blog.images.entity">
	<class name="ImgInfo" table="t_imgInfo">
		<id name="id" type="java.lang.String">
			<column name="id" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="imgUrl" type="java.lang.String">
			<column name="img_url" length="50" />
		</property>
		<property name="userId" type="java.lang.String">
			<column name="user_id" length="32" />
		</property>
		<property name="title" type="java.lang.String">
			<column name="title" length="20" />
		</property>
		<property name="context" type="java.lang.String">
			<column name="context" length="50" />
		</property>
	</class>

</hibernate-mapping>
	